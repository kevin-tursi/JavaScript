// function declaration
function newFunction() {

};

// function expression
var newFunction = function () {

};

// expression(s)
1 + 3;
var a = 2;
return true;

// calling/ invoking a function
alert();
newFunction(param1, param2);

// assign a variable
var a = true;

// function vs method
function thisIsAFunction() {

}

// vs. //

var obj = {
    thisIsAMethod: function () {

    }
}

thisIsAFunction();
obj.thisIsAMethod();